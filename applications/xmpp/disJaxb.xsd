    <xs:sequence/>
    <xs:attribute name="application" type="xs:int" use="required"/>
    <xs:attribute name="eventNumber" type="xs:int" use="required"/>
    <xs:attribute name="site" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="collisionPdu">
    <xs:complexContent>
      <xs:extension base="entityInformationFamilyPdu">
        <xs:sequence>
          <xs:element name="collidingEntityID" type="entityID" minOccurs="0"/>
          <xs:element name="eventID" type="eventID" minOccurs="0"/>
          <xs:element name="issuingEntityID" type="entityID" minOccurs="0"/>
          <xs:element name="location" type="vector3Float" minOccurs="0"/>
          <xs:element name="velocity" type="vector3Float" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="collisionType" type="xs:short" use="required"/>
        <xs:attribute name="mass" type="xs:float" use="required"/>
        <xs:attribute name="pad" type="xs:byte" use="required"/>
      </xs:extension>
