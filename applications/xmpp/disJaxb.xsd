        <xs:sequence>
          <xs:element name="collidingEntityID" type="entityID" minOccurs="0"/>
          <xs:element name="collisionEventID" type="eventID" minOccurs="0"/>
          <xs:element name="contactVelocity" type="vector3Float" minOccurs="0"/>
          <xs:element name="issuingEntityID" type="entityID" minOccurs="0"/>
          <xs:element name="location" type="vector3Float" minOccurs="0"/>
          <xs:element name="unitSurfaceNormal" type="vector3Float" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="coefficientOfRestitution" type="xs:float" use="required"/>
        <xs:attribute name="collisionResultXX" type="xs:float" use="required"/>
        <xs:attribute name="collisionResultXY" type="xs:float" use="required"/>
        <xs:attribute name="collisionResultXZ" type="xs:float" use="required"/>
        <xs:attribute name="collisionResultYY" type="xs:float" use="required"/>
        <xs:attribute name="collisionResultYZ" type="xs:float" use="required"/>
        <xs:attribute name="collisionResultZZ" type="xs:float" use="required"/>
        <xs:attribute name="mass" type="xs:float" use="required"/>
        <xs:attribute name="pad" type="xs:short" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="entityInformationFamilyPdu">
    <xs:complexContent>
      <xs:extension base="pdu">
        <xs:sequence/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

